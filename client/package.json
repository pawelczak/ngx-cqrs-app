{
	"name": "ngx-cqrs-app-client",
	"version": "1.0.0",
	"description": "Angular cqrs",
	"keywords": [
		"angular",
		"webpack",
		"karma",
		"jasmine",
		"starter"
	],
	"author": "Łukasz Pawełczak",
	"license": "MIT",
	"scripts": {
		"purge": "rimraf node_modules && rimraf dist",
		"clean": "rimraf dist",
		"clean:install": "yarn clean && rimraf node_modules && npm i",
		"prewebpack:build": "yarn clean",
		"webpack:build": "webpack --progress",
		"build": "yarn build:dev",
		"build:dev": "yarn webpack:build -- --config config/webpack.dev.js",
		"build:prod": "yarn webpack:build -- --config config/webpack.prod.js",
		"watch": "yarn webpack:build -- --config config/webpack.dev.js --watch",
		"server": "yarn server:dev",
		"server:dev": "webpack-dev-server --config config/webpack.dev.js --inline --progress --port 3000  --content-base src",
		"server:prod": "webpack-dev-server --config config/webpack.prod.js --inline --progress --port 3000  --content-base src",
		"start": "yarn server:dev",
		"start:prod": "yarn server:prod",
		"test": "karma start",
		"test:watch": "karma start --no-single-run --auto-watch",
		"analyze-bundles": "yarn webpack:build -- --config config/webpack.dev.js --profile --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json"
	},
	"dependencies": {
		"@angular/animations": "6.0.3",
		"@angular/cdk": "^6.3.3",
		"@angular/common": "6.0.3",
		"@angular/compiler": "6.0.3",
		"@angular/core": "6.0.3",
		"@angular/forms": "6.0.3",
		"@angular/http": "6.0.3",
		"@angular/material": "^6.0.0",
		"@angular/platform-browser": "6.0.3",
		"@angular/platform-browser-dynamic": "6.0.3",
		"@angular/platform-server": "6.0.3",
		"@angular/router": "6.0.3",
		"@ngrx/effects": "^6.0.0-beta.1",
		"@ngrx/store": "^6.0.0-beta.1",
		"@ngrx/store-devtools": "^6.0.1",
		"core-js": "^2.5.4",
		"es6-shim": "^0.35.1",
		"ie-shim": "^0.1.0",
		"jquery": "^3.0.0",
		"lodash": "4.17.4",
		"reflect-metadata": "^0.1.3",
		"rxjs": "^6.0.0",
		"zone.js": "^0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.6.0",
		"@angular/compiler-cli": "6.0.3",
		"@types/es6-shim": "0.0.31",
		"@types/jasmine": "^2.6.0",
		"@types/lodash": "ts2.0",
		"@types/node": "^6.0.38",
		"angular2-template-loader": "^0.6.2",
		"autoprefixer": "^6.5.1",
		"awesome-typescript-loader": "5.2.0",
		"copy-webpack-plugin": "^3.0.1",
		"css-loader": "^0.23.1",
		"exports-loader": "^0.6.3",
		"file-loader": "^0.9.0",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^0.14.3",
		"imports-loader": "^0.7.0",
		"jasmine-core": "^2.4.3",
		"karma": "^1.2.0",
		"karma-chrome-launcher": "^2.2.0",
		"karma-jasmine": "^1.0.2",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-webpack": "2.0.1",
		"node-sass": "^3.10.1",
		"null-loader": "^0.1.1",
		"precss": "^1.4.0",
		"raw-loader": "^0.5.1",
		"resolve-url-loader": "^1.6.1",
		"sass-loader": "^4.0.2",
		"style-loader": "^0.13.1",
		"to-string-loader": "^1.1.4",
		"ts-helpers": "^1.1.1",
		"ts-node": "^3.3.0",
		"tslib": "^1.7.1",
		"tslint": "^3.14.0",
		"tslint-loader": "^2.1.5",
		"typescript": "~2.7.2",
		"uglifyjs-webpack-plugin": "^1.2.7",
		"url-loader": "^0.5.7",
		"webpack": "4.15.1",
		"webpack-bundle-analyzer": "^2.9.2",
		"webpack-cli": "^3.0.8",
		"webpack-dev-server": "3.1.4",
		"webpack-merge": "4.1.0"
	}
}
